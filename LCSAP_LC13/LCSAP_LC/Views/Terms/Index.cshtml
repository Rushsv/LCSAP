@model IEnumerable<LCSAP_LC.Models.Term>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Term_Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Term_Start)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Term_End)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Term_Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Term_Start)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Term_End)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Term_Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Term_Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Term_Id })
        </td>
    </tr>
}

</table>
